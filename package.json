{
  "name": "solecheck-functions",
  "version": "1.0.0",
  "description": "Google Cloud Function for accessing the Shoes collection in Firestore",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "gcloud functions deploy accessShoesCollection --runtime nodejs20 --trigger-http --allow-unauthenticated"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.7.0",
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=18"
  }
}
